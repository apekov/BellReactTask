{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "lib": [
            "dom",
            "es2015",
            "es2016"
        ],
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "allowJs": true
    }
}
